package me.saac.i;

import pokerai.game.eval.spears.Card;

public class GameInfo {
	public enum Dealer { PLAYER, OPPONENT };
	
	public int smallBetSize;
	public Card[] playerHand;
	public Dealer dealer;
	
	public GameInfo(int sbs, Card[] ph, Dealer d) {
		smallBetSize = sbs;
		playerHand = ph;
		dealer = d;
	}
}
